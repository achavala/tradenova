<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.tradenova</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Users/chavala/TradeNova/venv/bin/python</string>
        <string>/Users/chavala/TradeNova/tradenova_daemon.py</string>
        <string>--paper</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Users/chavala/TradeNova</string>
    
    <key>StandardOutPath</key>
    <string>/Users/chavala/TradeNova/logs/tradenova_service.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/chavala/TradeNova/logs/tradenova_service_error.log</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
        <key>Crashed</key>
        <true/>
    </dict>
    
    <key>StartInterval</key>
    <integer>300</integer>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
    
    <key>ProcessType</key>
    <string>Background</string>
    
    <key>Nice</key>
    <integer>1</integer>
</dict>
</plist>

